{
  "extends": ["plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "no-alert": "warn",
    "indent": ["error", 2],
    "max-len": ["error", { "code": 120 }],
    "semi": ["error"],
    "quotes": ["error", "single"],
  },
}
